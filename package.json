{
  "name": "whiteboard-for-code",
  "displayName": "Whiteboard for Code",
  "description": "A simple whiteboard for VS Code",
  "version": "1.0.0",
  "engines": {
    "vscode": "^1.80.0"
  },
  "publisher": "Rohan-Shah-312003",
  "icon": "icon.png",
  "repository": {
    "type": "git",
    "url": "https://github.com/Rohan-Shah-312003/whiteboard-for-code.git"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "whiteboard.open",
        "title": "Open Whiteboard",
        "icon": "$(edit)"
      }
    ],
    "menus": {
      "editor/title": [
        {
          "command": "whiteboard.open",
          "when": "true",
          "group": "navigation"
        }
      ],
      "commandPalette": [
        {
          "command": "whiteboard.open",
          "title": "Whiteboard: Open"
        }
      ]
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/vscode": "^1.80.0",
    "@types/node": "18.x",
    "@typescript-eslint/eslint-plugin": "^6.13.0",
    "@typescript-eslint/parser": "^6.13.0",
    "eslint": "^8.54.0",
    "typescript": "^5.3.2"
  }
}
